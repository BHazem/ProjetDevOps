pipeline{
    agent any
    stages {
        stage("Testing Maven"){
            steps {
                sh "mvn -version"
            }
        }
        stage("Testing JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
              
            }


        }
      stage("Sonar"){
            steps {
                echo "sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar"
            }
    }
}
