pipeline{
    agent any
    stages {
        stage("Testing version  Maven"){
            steps {
                sh "mvn -version"
            }
        }
        stage("Testing version JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
            }
        }
        stage('MVN COMPILE') {
            steps{
                 dir('DevOps_Project') {
                sh 'mvn compile'
            }
        }
}
  stage('MVN test') {
            steps{
                 dir('DevOps_Project') {
                sh 'mvn test'
            }
        }
}

     stage('MVN SONARQUBE') {
            steps{
                 dir('DevOps_Project') {
                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar'
            }}
        }

          stage('Nexus') {
                    steps{
                         dir('DevOps_Project') {
                        sh 'mvn deploy'
                    }
                }
        }
    }
}